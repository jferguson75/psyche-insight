{
  "expo": {
    "name": "Psyche Insight",
    "slug": "psych-insight",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.psycheinsight.app"
    },
    "android": {
      "package": "com.psycheinsight.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png",
      "build": {
        "babel": {
          "include": ["@expo/vector-icons"]
        }
      }
    }
  }
}
